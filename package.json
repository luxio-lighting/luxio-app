{
  "name": "@luxio-lighting/app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build --platform all",
    "build:ios": "eas build --platform ios",
    "build:ios-simulator": "eas build --platform ios --profile development-simulator",
    "build:ios-simulator-local": "eas build --platform ios --profile development-simulator --local",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "cp:lib": "cp -r ../luxio-lib/ ./node_modules/@luxio-lighting/lib/"
  },
  "dependencies": {
    "@luxio-lighting/lib": "^2.1.2",
    "@react-native-community/slider": "^4.4.3",
    "expo": "~49.0.8",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.13",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-dialog": "^9.3.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-sse": "^1.2.1",
    "react-native-touchable-scale": "^2.2.0",
    "react-native-zeroconf": "^0.13.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eas-cli": "^5.2.0"
  },
  "private": true
}