{
  "name": "@luxio-lighting/app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build --platform all",
    "build:ios": "eas build --platform ios",
    "build:ios-simulator": "eas build --platform ios --profile development-simulator",
    "build:ios-simulator-local": "eas build --platform ios --profile development-simulator --local",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "cp:lib": "cp -r ../luxio-lib/ ./node_modules/@luxio-lighting/lib/"
  },
  "dependencies": {
    "@luxio-lighting/lib": "^2.1.3",
    "expo": "~50.0.14",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.3",
    "expo-haptics": "~12.8.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-dialog": "^9.3.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-sse": "^1.2.1",
    "react-native-touchable-scale": "^2.2.0",
    "react-native-zeroconf": "^0.13.8",
    "@react-native-community/slider": "4.4.2"
  },
  "overrides": {
    "@expo/prebuild-config": "~6.7.0",
    "@expo/config-plugins": "~7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eas-cli": "^7.6.2"
  },
  "private": true
}
